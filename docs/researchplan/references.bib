journal = {Genome research},
keywords = {Aggrecans,Aggrecans: genetics,Animals,Binding Sites,Binding Sites: genetics,Cartilage,Cartilage: embryology,Cartilage: growth \& development,Cartilage: metabolism,Chordata,Chordata: genetics,Ciona intestinalis,Ciona intestinalis: embryology,Ciona intestinalis: genetics,Ciona intestinalis: growth \& development,Computational Biology,Computational Biology: methods,Databases, Genetic,Evolution, Molecular,Gene Expression Profiling,Gene Expression Regulation, Developmental,In Situ Hybridization,Mutation,Notochord,Notochord: metabolism,Phylogeny,Proteins,Proteins: genetics,Recombination, Genetic,Tenascin,Tenascin: genetics,Transcription Factors,Transcription Factors: genetics,Urochordata,Urochordata: embryology,Urochordata: genetics,Urochordata: growth \& development,Vertebrates,Vertebrates: classification,Vertebrates: genetics},
